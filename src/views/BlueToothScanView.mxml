<s:View xmlns:fx="http://ns.adobe.com/mxml/2009" 
		xmlns:s="library://ns.adobe.com/flex/spark" title="BlueToothScanView">
	
	<fx:Script>
		<![CDATA[
			import mx.collections.ArrayCollection;
			/**
			 * list of devices found while scanning<br>
			 */
			[Bindable]
			private static var _scannedBluetoothDevices:ArrayCollection;
		]]>
	</fx:Script>
	<s:List id="bluetoothdevicelist" dataProvider="{_scannedBluetoothDevices}">
		
	</s:List>
</s:View>
